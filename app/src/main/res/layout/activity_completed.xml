<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompletedActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F5F5F5">
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:background="@drawable/back"
            android:layout_marginTop="54dp"
            android:layout_marginLeft="22dp"
            />
        <EditText
            android:layout_width="271dp"
            android:layout_height="43dp"
            android:background="@drawable/custom_input"
            android:hint="Search"
            android:paddingLeft="14dp"
            android:paddingRight="15.4dp"
            android:drawableEnd="@drawable/search"
            android:drawablePadding="50dp"
            android:layout_marginTop="52dp"
            android:layout_marginLeft="100dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="146dp">

            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="146dp"
                android:layout_height="41dp"
                android:layout_marginLeft="21dp"
                android:background="@drawable/bar"
                android:text="To Receive"
                android:textAllCaps="false"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="146dp"
                android:layout_height="41dp"
                android:layout_marginLeft="171dp"
                android:background="@drawable/bar2"
                android:text="Completed"
                android:textColor="@color/white"
                android:textAllCaps="false"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="90dp"
                android:layout_height="41dp"
                android:layout_marginLeft="321dp"
                android:background="@drawable/bar3"
                android:text="Canceled"
                android:textAllCaps="false"
                android:paddingLeft="30dp"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="28dp"
            android:layout_marginTop="206dp"
            android:layout_marginLeft="295dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="See all"
                android:textColor="#000"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="14dp"
                android:layout_height="9dp"
                android:background="@drawable/filter"
                android:layout_marginTop="6dp"
                android:layout_marginLeft="50dp"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="251dp"
            android:layout_centerHorizontal="true">
            <androidx.appcompat.widget.AppCompatImageButton
                android:layout_width="387dp"
                android:layout_height="138dp"
                android:background="@drawable/mobil1"/>
            <androidx.appcompat.widget.AppCompatImageButton
                android:layout_width="387dp"
                android:layout_height="138dp"
                android:layout_marginTop="150dp"
                android:background="@drawable/mobil2"/>
            <androidx.appcompat.widget.AppCompatImageButton
                android:layout_width="387dp"
                android:layout_height="138dp"
                android:layout_marginTop="300dp"
                android:background="@drawable/mobil3"/>
        </RelativeLayout>
    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout>