<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent" android:layout_height="wrap_content" >

	<ScrollView android:id="@+id/dashboard" 
		android:layout_width="wrap_content" 
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" >


			<RelativeLayout android:id="@+id/page_dashboard_ek1"
				android:layout_marginTop="0dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<View   android:id="@+id/_bg__dashboard_ek2"
					android:background="@drawable/_bg__dashboard_ek2_shape"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="410dp"
					android:layout_height="800dp" />

				<RelativeLayout android:id="@+id/dashboard_ek3"
					android:layout_marginTop="67dp"
					android:layout_marginStart="21dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					>

					<TextView android:id="@+id/dashboard_ek4"
						android:textStyle="bold"
						android:layout_gravity="center_horizontal|center"
						android:gravity="center"
						android:textSize="15sp"
						android:textColor="@color/_bg__dashboard_ek2_color"
						android:layout_marginLeft="2dp"
						android:layout_marginTop="0dp"
						android:layout_width="116.32dp"
						android:layout_height="33.83dp"
						android:fontFamily="@font/poppins_extra_bold"
						android:text="@string/dashboard_ek4_string"
						 />

					<androidx.appcompat.widget.AppCompatButton
						android:id="@+id/btn_seeall"
						android:layout_width="111.13dp"
						android:layout_height="33.33dp"
						android:textSize="13sp"
						android:textColor="@color/see_all_color"
						android:layout_marginLeft="246dp"
						android:layout_marginTop="341dp"
						android:text="@string/see_all_string"
						android:background="@null"
						android:textAllCaps="false"/>

					<TextView android:id="@+id/recomended"
						android:layout_gravity="center_horizontal|center"
						android:gravity="center"
						android:textSize="15sp"
						android:textColor="@color/_bg__dashboard_ek2_color"
						android:layout_marginLeft="5dp"
						android:layout_marginTop="340dp"
						android:layout_width="127.13dp"
						android:layout_height="34.33dp"
						android:text="@string/recomended_string"
						 />

					<ImageView android:id="@+id/line_5"
						android:src="@drawable/line_5"
						android:contentDescription="line_5"
						 android:layout_marginStart="4dp"
						android:layout_marginTop="26dp"
						android:layout_width="104.5dp"
						android:layout_height="0dp" />

					<ImageView android:id="@+id/line_6"
						android:src="@drawable/line_6"
						android:contentDescription="line_6"
						 android:layout_marginStart="8dp"
						android:layout_marginTop="364dp"
						android:layout_width="114.11dp"
						android:layout_height="0dp" />

					<ImageView android:id="@+id/__img___button_car"
						android:src="@drawable/__img___button_car"
						android:contentDescription="__img___button_car"
						 android:layout_marginStart="8dp"
						android:layout_marginTop="387dp"
						android:layout_width="360dp"
						android:layout_height="291.2dp" />

					<ImageView android:id="@+id/__img___button"
						android:src="@drawable/__img___button"
						android:contentDescription="__img___button"
						 android:layout_marginStart="0dp"
						android:layout_marginTop="51dp"
						android:layout_width="360dp"
						android:layout_height="242.61dp"
						/>

					<ImageView android:id="@+id/vector"
						android:src="@drawable/vector"
						android:contentDescription="vector"
						 android:layout_marginStart="321dp"
						android:layout_marginTop="1dp"
						android:layout_width="21.3dp"
						android:layout_height="20.61dp" />

				</RelativeLayout>

				<TextView
					android:id="@+id/customer_service"
					android:layout_width="70dp"
					android:layout_height="35dp"
					android:layout_marginLeft="103dp"
					android:layout_marginTop="285dp"
					android:text="@string/customer_service_string"
					android:textColor="@color/customer_service_color"
					android:textSize="15sp"
					android:textStyle="bold" />


				<RelativeLayout android:id="@+id/mdi_more_circle_outline"
					android:layout_marginTop="285dp"
					android:layout_marginStart="223dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					>

					<ImageView android:id="@+id/vector_ek1"
						android:src="@drawable/vector_ek1"
						android:contentDescription="vector_ek1"
						 android:layout_marginStart="3.75dp"
						android:layout_marginTop="3.75dp"
						android:layout_width="37.5dp"
						android:layout_height="37.5dp" />

				</RelativeLayout>

			</RelativeLayout>
		</RelativeLayout>

	</ScrollView>
</HorizontalScrollView>